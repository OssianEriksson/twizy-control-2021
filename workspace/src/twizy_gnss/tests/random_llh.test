<launch>
    <node name="random_llh_l" pkg="twizy_gnss" type="random_llh.py" args="--port 55556" />
    <node name="random_llh_r" pkg="twizy_gnss" type="random_llh.py" args="--port 55557" />

    <include file="$(find twizy_gnss)/launch/twizy_gnss.launch">
        <arg name="host_l" value="127.0.0.1" />
        <arg name="port_l" value="55556" />

        <arg name="host_r" value="127.0.0.1" />
        <arg name="port_r" value="55557" />
    </include>

    <test test-name="hztest_test" name="hztest1" pkg="rostest" type="hztest">
        <param name="topic" value="/gnss/pose" />
        <param name="hz" value="20.0" />
        <param name="hzerror" value="1.0" />
        <param name="test_duration" value="5.0" />
    </test>
</launch>